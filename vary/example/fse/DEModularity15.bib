@inbook{whittle2008a,
	title = {{MATA:} A Tool for {Aspect-Oriented} Modeling Based on Graph Transformation},
	url = {http://dx.doi.org/10.1007/978-3-540-69073-3_3},
	abstract = {This paper describes {MATA} {(Modeling} Aspects Using a Transfor-mation Approach), a {UML} aspect-oriented modeling tool that uses
graph transformations to specify and compose aspects. Graph transformations provide a unified approach for aspect modeling.
The methods presented here can be applied to any modeling language with a well-defined metamodel. This paper, however, focuses
on {UML} class diagrams, sequence diagrams and state diagrams. {MATA} takes a different approach to aspect-oriented modeling since
there are no explicit join points. Rather, any model element can be a join point and composition is a special case of model
transformation. We illustrate {MATA} on structural and behavioral models for a cell phone example.
},
	booktitle = {Models in Software Engineering},
	author = {Jon Whittle and Praveen Jayaraman},
