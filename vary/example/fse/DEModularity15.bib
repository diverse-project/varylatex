@incollection{sanchez2008,
	title = {Engineering Languages for Specifying {Product-Derivation} Processes in Software Product Lines},
	url = {http://dx.doi.org/10.1007/978-3-642-00434-6_13},
	abstract = {The goal of a Software Product Line {(SPL)} is to provide a set of reusable software assets for the rapid production of a software
systems family aimed at a specific market segment. The main objective of {SPL} engineering is to construct, as automatically
as possible, specific products after selecting the particular set of features that must be included in them. Unlike traditional
engineering of single systems, {SPL} engineering often requires dealing with three different languages at each stage of the
software lifecycle: (1) a language for specifying the variability of the {SPL} (e.g. a feature model); (2) a language for designing
the reusable software assets (e.g. {UML} 2.0); and (3) a language that specifies how these reusable assets must be composed
for constructing specific products. There are currently available enough languages for variability specification and software
assets design, but there is a general lack of languages for specifying and automating the composition of these assets. This
paper presents as a novel contribution a process to engineer this kind of language. The process produces an editor and a ÔøΩcompilerÔøΩ,
which automates the composition of reusable assets, for a particular language. To explain this process a language has been
developed to compose reusable assets of architectural models.
},
	booktitle = {Software Language Engineering (SLE)},
	author = {Pablo Sanchez and Neil Loughran and Lidia Fuentes and Alessandro Garcia},
	year = {2008},
	pages = {188--207}
}

